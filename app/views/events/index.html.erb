<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Mebog</th>
      <th>Fvs</th>
      <th>Date</th>
      <th>Eventable</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.mebog %></td>
        <td><%= event.fvs %></td>
        <td><%= event.date %></td>
        <td><%= event.eventable %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


