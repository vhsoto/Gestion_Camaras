<p id="notice"><%= notice %></p>

<h1>Listing Leaseds</h1>

<h4>Subir archivo plano de cámaras</h4>
<br>
<%= form_tag import_leaseds_path, multipart: true do %>
<div class="form-group">
  <div class="row">
    <div class="col-md-4 col-lg-4">
      <%= file_field_tag :file, required: true, class: "btn btn-info" %>
      <p class="help-block">Nota: La extensión del archivo debe ser .csv.</p>
    </div>
  </div>
</div>  
<%= submit_tag "cargar cámaras", class: "btn btn-info btn-sm" %>
<% end %>

<table class="table">
  <thead>
    <tr>
      <th>Spot</th>
      <th>Orientation</th>
      <th>Address</th>
      <th>Strangeness fvs</th>
      <th>Strangeness mebog</th>
      <th>Polygon</th>
      <th>Condition</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @leaseds.each do |leased| %>
      <tr>
        <td><%= leased.spot %></td>
        <td><%= leased.orientation %></td>
        <td><%= leased.address %></td>
        <td><%= leased.strangeness_fvs %></td>
        <td><%= leased.strangeness_mebog %></td>
        <td><%= leased.polygon %></td>
        <td><%= leased.condition %></td>
        <td><%= link_to 'Show', leased %></td>
        <td><%= link_to 'Edit', edit_leased_path(leased) %></td>
        <td><%= link_to 'Destroy', leased, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Leased', new_leased_path %>
